{
    "manifest_version": 2,
    "name": "Clear Cookie and Reload",
    "description": "Clear Cookie for current tab and reload",
    "version": "0.4",
    "permissions": [
        "cookies",
        "http://*/*",
        "https://*/*",
        "tabs",
        "contextMenus"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Clear Cookie and Reload"
    },
    "author": {
        "name": "Chirag",
        "email": "b.like.no.other@gmail.com"
    }
}
